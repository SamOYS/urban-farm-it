      "blocks": {
        "template--21440707952920__7db25f46-18f6-44e5-90f1-3093306ba23d-liquid-1": {
          "type": "liquid",
          "settings": {
            "code": "{% assign full_box = product.metafields.custom_product_variation.full_box %}\n{%- if full_box -%}\n{% assign full_box_var_url = product.metafields.custom_product_variation.full_box | prepend: '/products/' | prepend: shop.url %}\n    <script>\n        document.addEventListener('DOMContentLoaded', () => {\n            // Select the parent element to which we want to append the new variant option\n            const parentElement = document.querySelector('fieldset.variant-input-wrap[data-handle=\"amount\"]');\n            \n            if (parentElement) {\n                // Create the new variant option element\n                const variantInputDiv = document.createElement('div');\n                variantInputDiv.className = 'variant-input';\n\n                const anchorTag = document.createElement('a');\n                anchorTag.href = \"{{ full_box_var_url }}\";\n                anchorTag.textContent = 'Full Box';\n\n                // Style the anchor tag\n                anchorTag.style.position = 'relative';\n                anchorTag.style.display = 'inline-block';\n                anchorTag.style.fontWeight = '400';\n                anchorTag.style.padding = '7px 15px';\n                anchorTag.style.margin = '0 8px 12px 0';\n                anchorTag.style.backgroundColor = '#fff'; // Fallback color\n                anchorTag.style.backgroundColor = 'var(--colorBody)';\n                anchorTag.style.boxShadow = '0 0 0 1px #2b2a27'; // Fallback shadow\n                anchorTag.style.boxShadow = '0 0 0 1px var(--colorBorder)';\n                anchorTag.style.overflow = 'hidden';\n\n                // Append the anchor tag to the variant input div\n                variantInputDiv.appendChild(anchorTag);\n\n                // Append the variant input div to the parent element\n                parentElement.appendChild(variantInputDiv);\n            }\n        });\n    </script>\n    {% else %}\n    {%- for item in collections['mushroom-growing-containers'].products -%}\n        {%- assign item_has_full_box_metafield = item.metafields.custom_product_variation.full_box -%}\n        {%- if item_has_full_box_metafield -%}\n            {%- if item_has_full_box_metafield == product.handle -%}\n                {% if item.options.size > 0 -%}\n                    <script>\n                        document.addEventListener('DOMContentLoaded', () => {\n                            const parentElement = document.querySelector('fieldset.variant-input-wrap[data-handle=\"amount\"]');\n                            console.log(parentElement)\n                            const validOptions = [\n                                {%- for var in item.variants -%}\n                                {%- assign valid_options = item.options_by_name['amount'].values -%}\n                                {%- if valid_options contains var.option1 -%}\n                                {'url': '{{ var.url }}', 'textContent': '{{ var.option1 }}'}{% unless forloop.last %},{% endunless %}\n                                {%- endif -%}\n                            {%- endfor -%}]\n\n                            for (let i = validOptions.length - 1; i >= 0; i--) {\n                                let { url, textContent } = validOptions[i];\n                                const variantInputDiv = document.createElement('div');\n                                variantInputDiv.className = 'variant-input';\n\n                                const anchorTag = document.createElement('a');\n                                anchorTag.href = url;\n                                anchorTag.textContent = textContent;\n\n                                // Style the anchor tag\n                                anchorTag.style.position = 'relative';\n                                anchorTag.style.display = 'inline-block';\n                                anchorTag.style.fontWeight = '400';\n                                anchorTag.style.padding = '7px 15px';\n                                anchorTag.style.margin = '0 8px 12px 0';\n                                anchorTag.style.backgroundColor = '#fff'; // Fallback color\n                                anchorTag.style.backgroundColor = 'var(--colorBody)';\n                                anchorTag.style.boxShadow = '0 0 0 1px #2b2a27'; // Fallback shadow\n                                anchorTag.style.boxShadow = '0 0 0 1px var(--colorBorder)';\n                                anchorTag.style.overflow = 'hidden';\n\n                                // Append the anchor tag to the variant input div\n                                variantInputDiv.appendChild(anchorTag);\n\n                                // Append the variant input div to the parent element\n                                parentElement.prepend(variantInputDiv);\n                            }\n                        });\n                    </script>\n                {%- endif %}\n            {%- endif -%}\n        {%- endif -%}\n    {%- endfor -%}\n{%- endif -%}",
            "width": "100%",
            "alignment": "center"
          }
        }
